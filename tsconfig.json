{
  "compilerOptions": {
    // 允许编译javascript文件
    "allowJs": true,
    // 不报告执行不到的代码错误
    "allowUnreachableCode": true,
    // 不报告未使用的标签错误
    "allowUnusedLabels": true,
    //
    "jsx": "react",
    // 生成.js文件版本
    "target": "es5",
    "module": "commonjs",
    // 启用实验性的ES装饰器
    "experimentalDecorators": true,
    // 显示诊断信息
    "diagnostics": true
  }
}